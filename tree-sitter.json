{
  "grammars": [
    {
      "name": "gotmpl",
      "camelcase": "GoTmpl",
      "scope": "source.gotmpl",
      "file-types": [
        ".tpl",
        ".gotmpl"
      ],
      "injection-regex": "^gotmpl$",
      "path": "gotmpl"
    },
    {
      "name": "helm",
      "camelcase": "Helm",
      "scope": "source.helm",
      "file-types": [
        ".tpl",
        ".gotmpl",
        ".yaml"
      ],
      "injection-regex": "^helm$",
      "path": "helm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Go template grammar for tree-sitter",
    "authors": [
      {
        "name": "infastin"
      }
    ],
    "links": {
      "repository": "https://github.com/infastin/tree-sitter-gotmpl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
